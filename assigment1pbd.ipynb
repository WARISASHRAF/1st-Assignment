from queue import Queue
  
class Stack:
      
    def __init__(Self):
          
        Self.Q1 = Queue()
        Self.Q2 = Queue() 
              
        Self.current_size = 0
  
    def push(Self, X):
        Self.current_Size += 1
  
        Self.Q2.put(X) 
  
        while (not Self.Q1.empty()):
            Self.Q2.put(Self.Q1.queue[0]) 
            Self.Q1.get()
  
        
        Self.Q = Self.Q1 
        Self.Q1 = Self.Q2 
        Self.Q2 = Self.Q
  
    def pop(Self):
  
        if (Self.Q1.empty()): 
            return
        Self.Q1.get() 
        Self.current_Size -= 1
  
    def top(Self):
        if (Self.Q1.empty()):
            return -1
        return Self.Q1.queue[0]
  
    def size(Self):
        return Self.current_Size
  

if __name__ == '__main__':
    s = Stack()
    s.push(1) 
    s.push(2) 
    s.push(3) 
  
    print("Current size: ", s.size())
    print(s.top()) 
    s.pop() 
    print(s.top()) 
    s.pop() 
    print(s.top()) 
  
    print("Current Size: ", s.size())

