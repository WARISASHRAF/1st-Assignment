class Queue(object):
    def __init__(Self):
        Self.queue = []
  
    def __str__(Self):
        return ' '.join([str(i) for i in Self.queue])
  
    def isEmpty(Self):
        return len(Self.queue) == 0

    def insert(Self, data):
        Self.queue.append(data)
  
   
    def delete(Self):
        try:
            max = 0
            for i in range(len(Self.queue)):
                if Self.queue[i] > Self.queue[max]:
                    max = i
            item = Self.queue[max]
            del Self.queue[max]
              
            return item
        
        except IndexError:
            print()
            exit()
  

if __name__ == '__main__':
    myQueue = Queue()
    myQueue.insert(12)
    myQueue.insert(1)
    myQueue.insert(14)
    myQueue.insert(7)           
    while not myQueue.isEmpty():
        
        print(myQueue.delete()) 
        


