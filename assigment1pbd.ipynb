def isPrime(n):
    if n in {2, 3, 5, 7}:
        return True
    if n < 2 or n%2 == 0:
        return False
    if n%3 == 0 or n%5 == 0:
        return False
    r = int(n**0.5)
    a = 5
    while a <= r:
        if n%a == 0 or n%(a+2) == 0:
            return False
        a += 6
    return True

def gen(nb):
    yield nb
    nb_str = str(nb)
    for j in range(1, len(nb_str)):
        yield int(nb_str[j:])
        yield int(nb_str[:-j])

def check(nb):
    for t in gen(nb):
        if not isPrime(t):
            return False
    return True

